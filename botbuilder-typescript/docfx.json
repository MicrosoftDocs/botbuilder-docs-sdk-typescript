{
  "build": {
    "content": [
       {
        "files": [
          "**/toc.yml",
          "**/toc.md"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/bb_ts_toc",
        "version": "botbuilder-ts-latest"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
           "**/toc.yml"
        ],
        "dest":"api",
        "version": "botbuilder-ts-latest"
      },
	  {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "v3/docs-ref-autogen",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
           "**/toc.yml"
        ],
        "dest":"api",
        "version": "botbuilder-ts-3.0"
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "api/bb_ts_breadcrumb",
        "version": "botbuilder-ts-latest",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "version": "botbuilder-ts-latest"
      }
    ],
    "versions": {
      "botbuilder-ts-latest": {
        "dest": "botbuilder-ts-latest"
      },
	  "botbuilder-ts-3.0": {
        "dest": "botbuilder-ts-3.0"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "javascript",
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },
    "fileMetadata": {
      "monikers": {
        "docs-ref-autogen/**": [
          "botbuilder-ts-latest"
        ],
		"v3/docs-ref-autogen/**": [
          "botbuilder-ts-3.0"
        ]
      }
    },
    "template": [],
    "dest": "_site"
  }
}

